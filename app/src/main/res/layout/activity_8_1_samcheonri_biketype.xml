<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_samcheonri_bike_9"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.ex.bikeencyclopediaapplication.activity.Samcheonri_Biketype_8_1_Activity">


    <Button
        android:id="@+id/samcheonri_road_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="24dp"
        android:background="#96ff1744"
        android:text="R O A D"
        android:textSize="24sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/samcheonri_mtb_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="24dp"
        android:background="#96ff1744"
        android:text="M T B"
        android:textSize="24sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/samcheonri_electric_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="24dp"
        android:background="#96ff1744"
        android:text="ELECTRIC"
        android:textSize="24sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/samcheonri_fixie_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="24dp"
        android:background="#96ff1744"
        android:text="F I X I E"
        android:textSize="24sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/samcheonri_minivelo_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="24dp"
        android:background="#96ff1744"
        android:text="MINI VELO"
        android:textSize="24sp"
        android:textStyle="bold" />


</LinearLayout>
